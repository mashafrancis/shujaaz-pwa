{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "add-module-exports",
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "@component": "./src/components/",
          "@pages": "./src/pages/",
          "@placeholders": "./src/placeholders/",
          "@modules": "./src/store/modules/",
          "@context": "./src/context/"
        }
      }
    ]
  ]
}
